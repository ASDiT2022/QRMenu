{
  "name": "tmr_datawarehouse_backend",
  "version": "0.0.1",
  "main": "src/index.ts",
  "repository": "git@github.com:kelloTMR/tmr-datawarehouse-back-end.git",
  "author": "",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^14.13.0"
  },
  "scripts": {
    "prestart": "npm run migrate",
    "start": "cross-env node -r dotenv/config -r ts-node/register src/index.ts",
    "apidoc": "cross-env node -r ts-node/register scripts/apidoc.ts",
    "test": "cross-env NODE_ENV=test JWT_SECRET=somerandomsecret ts-mocha --config \"./.mocharc.js\"",
    "test:debug": "cross-env NODE_ENV=test JWT_SECRET=somerandomsecret ts-mocha --config \"./.mocharc.js\" --inspect=9999",
    "test:coverage": "cross-env NODE_ENV=test JWT_SECRET=somerandomsecret nyc --reporter=lcov --temp-dir=\"./temp/.nyc_output\" ts-mocha --config \"./.mocharc.js\" --timeout=100000",
    "lint": "eslint src tests --color --ext .js --ext .ts",
    "lint-fix": "eslint src tests --color --ext .js --ext .ts --fix",
    "translate-scanner": "gulp translate-scanner",
    "todo-scanner": "gulp todo-scanner",
    "migrate": "cross-env node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "migrate:undo": "cross-env node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "seed:dev": "cross-env node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "seed:dev:debug": "cross-env node --inspect=8888 -r dotenv/config -r ts-node/register scripts/devseed.ts",
    "seed:dev:undo": "cross-env node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all",
    "debug:script": "cross-env NODE_ENV=development nodemon --exec 'node --inspect -r dotenv/config -r ts-node/register src/index.ts'",
    "predebug": "npm run migrate",
    "debug": "npm-run-all -p todo-scanner translate-scanner debug:script",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "joi-types": "ts-node scripts/joi-to-ts.ts",
    "create-trigger-files": "ts-node scripts/create-log-triggers.ts",
    "install-client": "cd ../FE/qr-menu && npm install",
    "heroku-postbuild": "npm install && npm run install-client && npm run build"
  },
  "dependencies": {
    "@goodrequest/eslint-config-typescript": "1.2.0",
