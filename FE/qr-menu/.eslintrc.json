{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier"
  ],
  "overrides": [
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "import/no-extraneous-dependencies": "off",
    "react/react-in-jsx-scope": "off",
    "no-console": "warn"
  },
  "ignorePatterns": "*.json"
}
